<?xml version="1.0" ?>
<sdf version="1.4">
  <!-- We use a custom world for the rrbot so that the camera angle is launched correctly -->

  <world name="default">

		<include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <pose>-17 -17 0 0 0 0</pose>
      <uri>model://willowgarage</uri>
    </include>

    <!-- Global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

  </world>
</sdf>
