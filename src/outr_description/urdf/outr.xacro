<?xml version='1.0'?>

<robot name="robot" 
  xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find outr_description)/urdf/outr.gazebo" />
  <xacro:include filename="$(find outr_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find outr_description)/urdf/macros.xacro" />
  <xacro:include filename="$(find outr_description)/urdf/_d435.urdf.xacro"/>

  <link name='chassis'>
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.15 0 -0.15" rpy=" 0 0 3.14159"/>
      <inertia ixx="100.0" ixy="0" ixz="0" iyy="100.0" iyz="0" izz="100.0" />
    </inertial>

    <collision name='collision'>
      <origin xyz="0.15 0 -0.15" rpy=" 0 0 3.14159"/>
      <geometry>
        <mesh filename="package://outr_description/urdf/outr.dae"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0.15 0 -0.15" rpy=" 0 0 3.14159"/>
      <geometry>
        <mesh filename="package://outr_description/urdf/outr.dae"/>
      </geometry>
    </visual>


    <collision name='caster1_collision'>
      <origin xyz="0.45 0.22 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>5.0</slip1>
            <slip2>5.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='caster1_visual'>
      <origin xyz="0.45 0.22 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>

    <collision name='caster2_collision'>
      <origin xyz="0.45 -0.22 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>5.0</slip1>
            <slip2>5.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='caster2_visual'>
      <origin xyz="0.45 -0.22 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>

    <collision name='caster3_collision'>
      <origin xyz="-0.45 0.22 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>5.0</slip1>
            <slip2>5.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='caster3_visual'>
      <origin xyz="-0.45 0.22 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>

    <collision name='caster4_collision'>
      <origin xyz="-0.45 -0.22 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>5.0</slip1>
            <slip2>5.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='caster4_visual'>
      <origin xyz="-0.45 -0.22 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>

  </link>


  <link name="left_wheel">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.03"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>50</mu>
            <mu2>50</mu2>
            <slip1>0</slip1>
            <slip2>0</slip2>
          </ode>
        </friction>
        <contact>
          <ode>
            <min_depth>10</min_depth>
          </ode>
        </contact>
      </surface>
    </collision>
    <visual name="left_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.03"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.1" h="0.03"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <link name="right_wheel">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.03"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>50</mu>
            <mu2>50</mu2>
            <slip1>0</slip1>
            <slip2>0</slip2>
          </ode>
        </friction>
        <contact>
          <ode>
            <min_depth>10</min_depth>
          </ode>
        </contact>
      </surface>
    </collision>
    <visual name="right_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.03"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.1" h="0.03"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <gazebo reference="right_wheel">
    <material>WheelMaterial</material>
  </gazebo>
  <gazebo reference="left_wheel">
    <material>WheelMaterial</material>
  </gazebo>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 0.22 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100000000" velocity="100000000"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 -0.22 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100000000" velocity="100000000"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>

  <sensor_d435 parent="chassis">
    <origin xyz="0.63 0 0" rpy="0 0 0"/>
  </sensor_d435>

</robot>
